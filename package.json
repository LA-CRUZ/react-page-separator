{
	"name": "@lacruz/react-page-separator",
	"version": "0.0.2",
	"description": "react-page plugin adding a customisable horizontal separator",
	"author": "lacruz",
	"keywords": [
		"react",
		"react-page",
		"plugins",
		"separator"
	],
	"license": "MIT",
	"homepage": "https://github.com/LA-CRUZ/react-page-separator#readme",
	"bugs": {
		"url": "https://github.com/LA-CRUZ/react-page-separator/issues"
	},
	"main": "./dist/react-page-separator.umd.js",
	"module": "./dist/react-page-separator.es.js",
	"exports": {
		".": {
			"import": "./dist/react-page-separator.es.js",
			"require": "./dist/react-page-separator.umd.js"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"serve": "vite preview",
		"lint:fix": "eslint ./src --ext .jsx,.js --quiet --fix --ignore-path ./.gitignore",
		"lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,css,sass,md,json}\" ",
		"lint": "yarn lint:format && yarn lint:fix"
	},
	"dependencies": {
		"@emotion/react": "^11.8.1",
		"@emotion/styled": "^11.8.1",
		"@mui/icons-material": "^5.4.2",
		"@mui/material": "^5.4.3",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@vitejs/plugin-react": "^1.2.0",
		"react": "^17.0.0",
		"react-dom": "^17.0.2",
		"vite": "^2.2.3",
		"vite-plugin-react-svg": "^0.2.0"
	},
	"devDependencies": {
		"babel-plugin-styled-components": "^2.0.2",
		"eslint": "^7.22.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "^7.22.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"prettier": "^2.2.1",
		"sass": "^1.32.12"
	}
}
